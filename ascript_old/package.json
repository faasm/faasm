{
  "scripts": {
    "asbuild:untouched": "asc assembly/$FUNC.ts -b build/$FUNC.untouched.wasm -t build/$FUNC.untouched.wast --validate --debug",
    "asbuild:optimized": "asc assembly/$FUNC.ts -b build/$FUNC.optimized.wasm -t build/$FUNC.optimized.wast --validate --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "echo": "FUNC=echo npm run asbuild"
  },
  "devDependencies": {
    "assemblyscript": "github:AssemblyScript/assemblyscript"
  }
}
